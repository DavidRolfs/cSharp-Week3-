@if (@Model["Client"].Count == 0)
{
  <p>No Clients have been added yet!</p>
}
else
{
  <ol>
    @foreach (var client in @Model["Client"])
    {
      <li>
      <a href="/client/@client.GetId()">@client.GetName()</a>
      <a href="/client/edit/@client.GetId()">Edit</a>
      <a href="/client/delete/@client.GetId()">Delete</a></li>
      </li>
    }
  </ol>
}


